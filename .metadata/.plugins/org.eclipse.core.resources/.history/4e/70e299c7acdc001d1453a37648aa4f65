package br.com.unesp.tecnicas_de_programacao;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class URI_1244 {

	public static void main(String[] args) {
		try (Scanner teclado = new Scanner(System.in)) {
			System.out.println("Digite quanto conjuntos de string voce ir√° digitar: ");
			int N = teclado.nextInt();
			
			for (int i = 0; i < N; i++) {
				String linha = teclado.nextLine();
				String espaco[] = linha.split(" ");
				List<String> lista = new ArrayList<>();

			for (int j = 0; j<espaco.length;j++) {
				lista.add(espaco[j]);
			}
			
			while (!lista.isEmpty()) {
				String maior = "";
				for (String x : lista) {
					if(x.length() > maior.length()) {
						maior = x;
						System.out.printf("%s",maior);
			        if(lista.contains(maior))  //antes usava while
			           lista.remove(maior);
			         
			        if(!lista.isEmpty()) 
			        	System.out.printf(" ");
			        }
					System.out.println("");
			}
			teclado.close();
					}
			
				
				
			}
			
		}
	
		
	
	}

}
